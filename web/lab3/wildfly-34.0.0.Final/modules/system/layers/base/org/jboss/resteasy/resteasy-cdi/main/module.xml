<module name="org.jboss.resteasy.resteasy-cdi" xmlns="urn:jboss:module:1.9">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <resource-root path="resteasy-cdi-6.2.10.Final.jar"/>
    </resources>

    <dependencies>
        <module name="java.naming"/>
        <module name="jakarta.annotation.api"/>
        <module name="jakarta.enterprise.api"/>
        <module name="jakarta.servlet.api"/>
        <module name="jakarta.ws.rs.api"/>
        <module name="jakarta.validation.api"/>
        <module name="org.jboss.resteasy.resteasy-core"/>
        <module name="org.jboss.resteasy.resteasy-core-spi"/>
        <module name="org.jboss.weld.core"/>
        <module name="org.jboss.weld.spi"/>
        <module name="org.jboss.logging"/>
        <module name="org.eclipse.microprofile.restclient" optional="true"/>
    </dependencies>
</module>
