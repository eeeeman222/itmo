<module name="org.infinispan.hibernate-cache" xmlns="urn:jboss:module:1.9">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>


    <resources>
        <resource-root path="infinispan-hibernate-cache-spi-14.0.32.Final.jar"/>
        <resource-root path="infinispan-hibernate-cache-commons-14.0.32.Final.jar"/>
        <resource-root path="infinispan-hibernate-cache-v62-14.0.32.Final.jar"/>
    </resources>

    <dependencies>
        <module name="java.naming"/>
        <module name="java.sql"/>
        <module name="jakarta.transaction.api"/>

        <module name="org.hibernate"/>
        <module name="org.hibernate.jipijapa-hibernate6" services="import"/>
        <module name="org.infinispan.commons"/>
        <module name="org.infinispan.core" services="import"/>
        <module name="org.jboss.as.jpa.spi"/>
        <module name="org.jboss.logging"/>
        <module name="org.wildfly.clustering.cache.infinispan.embedded" services="import"/>
    </dependencies>
</module>
