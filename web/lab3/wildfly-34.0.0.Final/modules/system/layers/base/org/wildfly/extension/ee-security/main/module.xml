<module name="org.wildfly.extension.ee-security" xmlns="urn:jboss:module:1.9">

    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <exports>
        <exclude path="org/wildfly/extension/ee-security/logging"/>
    </exports>

    <resources>
        <resource-root path="wildfly-ee-security-34.0.0.Final.jar"/>
    </resources>

    <dependencies>
        <module name="org.jboss.staxmapper"/>
        <module name="org.jboss.as.controller"/>
        <module name="org.jboss.as.ee"/>
        <module name="org.jboss.as.server"/>
        <module name="org.jboss.modules"/>
        <module name="org.jboss.msc"/>
        <module name="org.jboss.logging"/>
        <module name="org.glassfish.soteria"/>
        <module name="jakarta.security.enterprise.api"/>
        <module name="jakarta.enterprise.api"/>
        <module name="io.smallrye.jandex"/>
    </dependencies>
</module>
